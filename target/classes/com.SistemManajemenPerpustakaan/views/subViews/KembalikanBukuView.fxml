<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="500.0" prefWidth="600.0" spacing="15.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.SistemManajemenPerpustakaan.MVC.Views.GUI.subController.KembalikanBukuController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <children>
        <Label text="Kembalikan Buku">
            <font>
                <Font name="System Bold" size="18.0" />
            </font>
        </Label>
        <Label text="Berikut adalah daftar buku yang sedang Anda pinjam:" />
        <ListView fx:id="listViewPinjamanAktif" VBox.vgrow="ALWAYS" />
    </children>
</VBox>